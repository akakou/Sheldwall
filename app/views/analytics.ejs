<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>Sheld Controll &ndash; Analytics &ndash;</title>

  <script src="static/lib/chart.js"></script>
  <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">
  <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/grids-responsive-min.css">

  <link rel="stylesheet" href="https://unpkg.com/purecss@1.0.0/build/pure-min.css" integrity="sha384-nn4HPE8lTHyVtfCBi5yW9d20FjT8BJwUXyWZT9InLYax14RDjBj46LmSztkmNP9w" crossorigin="anonymous">

  <link rel="stylesheet" href="static/style/main.css"></link>
  <link rel="stylesheet" href="static/style/size.css"></link>
  <link rel="stylesheet" href="static/style/content.css"></link>
  <link rel="stylesheet" href="static/style/table.css"></link>
  <link rel="stylesheet" href="static/style/layout.css"></link>

  <script src="static/script/chart.js"></script>
  <script src="static/script/show.js"></script>
</head>
<body>





<div class="pure-menu pure-menu-horizontal">
  <a href="#" class="pure-menu-heading">Sheld Controller</a>
</div>

<div class="banner">
  <h1 class="banner-head">
    This is the result <br>
    Analytics The Packets
  </h1>
</div>

<h1 class="content-head">this month score</h1>



<div class="l-content">
  <div class="figure-tables pure-g">
    <div class="pure-u-1">
      <div class="figure-table figure-table-access">
        <div class="figure-table-header">
          <h2>Access Count</h2>

          <span class="figure-table-figure">
            <div id="all_access_count"></div> <span>per month</span>
          </span>
        </div>

        <button class="button-choose pure-button" onclick="show_access_count()">Check More</button>
      </div>
    </div>

    <div class="pure-u-1">
      <div class="figure-table figure-table-block">
        <div class="figure-table-header">
          <h2>Block Count</h2>

          <span class="figure-table-figure">
            <div id="all_block_count"></div> <span>per month</span>
          </span>
        </div>

        <button class="button-choose pure-button" onclick="show_access_block()">Choose</button>
      </div>
    </div>

    <div class="pure-u-1">
      <div class="figure-table figure-table-biz figure-table-selected">
        <div class="figure-table-header">
          <h2>Accessed Destination</h2>

          <span class="figure-table-figure">
            <div style="word-wrap: break-word; font-size: 70%; " id="most_accessed_destination">ã€€</div> <span>is the most accessed website</span>
          </span>
        </div>

        <button class="button-choose pure-button" onclick="show_access_destination()">Choose</button>
      </div>
    </div>

    <div class="pure-u-1">
      <div class="figure-table figure-table-enterprise">
        <div class="figure-table-header">
          <h2>Blocked Destination</h2>

          <span class="figure-table-figure">
            <div style="word-wrap: break-word; font-size: 70%; " id="most_blocked_destination"></div> <span>is the most blocked website</span>
          </span>
        </div>

        <button class="button-choose pure-button" onclick="show_block_destination()">Choose</button>
      </div>
    </div>
  </div> <!-- end figure-tables -->
</div> <!-- end l-content -->

<h1 class="content-head">See with the figure</h1>

<div class="l-content">
  <div class="figure-tables pure-g">
    <div class="pure-u-1">
      <div style="padding: width:50% height:50%;" id="charts">
        <canvas id="canvas" name="canvas"></canvas>
      </div>
    </div>
  </div>
</div>




<div class="footer l-box">
  <p>
    Sheld is made by sheld developers.

    If you find the bugs, please tell it to
    <a href="https://twitter.com/akakou_py">@akakou_py</a>.
  </p>
</div>


<div id="access-count">
  <% var count = 0; %>
  <% access_time.forEach(function (value) { %>
    <input type="hidden"
      id="<%= 'access-count-' + count %>"
      value="<%= value.key + ',' + value.count %>"
    >
    </input>
    <% count++ %>
  <% }); %>
</div>

<div id="access-destionation">
  <% var count = 0; %>
  <% destination.forEach(function (value) { %>
    <input type="hidden"
      id="<%= 'access-destionation-' + count %>"
      value="<%= value.key + ',' + value.count %>"
    >
    </input>
    <% count++ %>
  <% }); %>
</div>

<div id="block-count">
  <% var count = 0; %>
  <% block_time.forEach(function (value) { %>
    <input type="hidden"
      id="<%= 'block-count-' + count %>"
      value="<%= value.key + ',' + value.count %>"
    >
    </input>
    <% count++ %>
  <% }); %>
</div>

<div id="block-destination">
  <% var count = 0; %>
  <% block_destination.forEach(function (value) { %>
    <input type="hidden"
      id="<%= 'block-destination-' + count %>"
      value="<%= value.key + ',' + value.count %>"
    >
    </input>
    <% count++ %>
  <% }); %>
</div>

<script>
  show_all_access_count();
  show_all_block_count();
  show_most_accessed_destination();
  show_most_blocked_destination();
</script>>

</body>
</html>
